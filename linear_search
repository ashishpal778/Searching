#include <iostream>
using namespace std;
int linear_search(int ar[],int n,int x){
    int i;
    for(i=0;i<n;i++){
        if(x==ar[i])
        return i;
    }
    return 0;
}
int main(){
    int n,x;
    cin>>n>>x;
    int ar[n];
    for(int i=0;i<n;i++){
        cin>>ar[i];
    }
     cout<<linear_search(ar,n, x);
}
